{
  "name": "@cloneoverflow/common",
  "version": "1.0.25",
  "description": "",
  "main": "./dist/build/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "del-cli ./dist/*",
    "build": "npm run clean && tsc -p tsconfig.build.json && tsc -p tsconfig.types.json && tsc-alias -p tsconfig.build.json && tsc-alias -p tsconfig.types.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/build/index.js"
    },
    "./api": {
      "types": "./dist/types/api/http/index.d.ts",
      "require": "./dist/build/api/http/index.js"
    },
    "./api/*": {
      "types": "./dist/types/api/http/*/index.d.ts",
      "require": "./dist/build/api/http/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "api": [
        "./dist/types/api/http/index.d.ts"
      ],
      "api/*": [
        "./dist/types/api/http/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "del-cli": "^5.1.0",
    "reflect-metadata": "^0.2.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@nestjs/swagger": "^11.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2"
  }
}
